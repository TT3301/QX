[Script]
# == 去开屏•12306 ▶ %APPEND% ad.12306.cn
去开屏•12306 = type=http-request, pattern=^https?:\/\/ad\.12306\.cn\/ad\/ser\/getAdList, script-path=https://raw.githubusercontent.com/TT3301/QX/main/rewrite/12306.js, requires-body=true
